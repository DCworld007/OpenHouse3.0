    "@prisma/adapter-d1": "^6.7.0",
    "@swc/helpers": "^0.5.17",
    "@types/node": "20.6.2",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "10.4.15",
    "bcryptjs": "^2.4.3",
    "cookie": "^1.0.2",
