    "eslint": "^8.49.0",
    "eslint-config-next": "^15.3.1",
    "framer-motion": "^12.9.2",
    "fuzzysort": "^3.1.0",
    "google-auth-library": "^9.15.1",
    "jose": "^4.15.9",
    "js-base64": "^3.7.7",
    "jsonwebtoken": "^9.0.2",
